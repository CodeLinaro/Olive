{
    "input_model": {
        "type": "HfModel",
        "model_path": "google-bert/bert-base-multilingual-cased",
        "task": "feature-extraction"
    },
    "systems": {
        "qnn_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "npu", "execution_providers": [ "QNNExecutionProvider" ] } ]
        }
    },
    "data_configs": [
        {
            "name": "xnli",
            "type": "HuggingfaceContainer",
            "load_dataset_config": { "data_name": "facebook/xnli", "subset": "en", "split": "validation" },
            "pre_process_data_config": {
                "input_cols": [ "premise" ],
                "padding": "max_length",
                "max_length": 512,
                "max_samples": 10
            },
            "dataloader_config": { "batch_size": 1 }
        }
    ],
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "latency_qnn",
                    "type": "latency",
                    "data_config": "xnli",
                    "sub_types": [
                        { "name": "avg", "priority": 1, "goal": { "type": "percent-min-improvement", "value": 0.1 } }
                    ],
                    "user_config": {
                        "inference_settings": {
                            "onnx": {
                                "session_options": {
                                    "extra_session_config": { "session.disable_cpu_ep_fallback": "0" }
                                },
                                "execution_provider": "QNNExecutionProvider",
                                "provider_options": [
                                    { "backend_path": "QnnHtp.dll", "htp_performance_mode": "burst" }
                                ]
                            }
                        }
                    }
                }
            ]
        }
    },
    "passes": {
        "conversion": { "type": "OnnxConversion", "target_opset": 20 },
        "dynamic_shape_to_fixed": {
            "type": "DynamicToFixedShape",
            "dim_param": [ "batch_size", "sequence_length" ],
            "dim_value": [ 1, 512 ]
        },
        "surgery": {
            "type": "GraphSurgeries",
            "surgeries": [ { "surgeon": "ReplaceAttentionMaskValue" }, { "surgeon": "MatMulAddToGemm" } ]
        },
        "quantization": {
            "type": "OnnxStaticQuantization",
            "data_config": "xnli",
            "activation_type": "QUInt16",
            "weight_type": "QUInt8",
            "calibrate_method": "MinMax",
            "quant_preprocess": true,
            "prepare_qnn_config": true
        },
        "qnn_preprocess": { "type": "QNNPreprocess" },
        "cb": {
            "type": "EPContextBinaryGenerator",
            "provider_options": {
                "htp_graph_finalization_optimization_mode": "3",
                "offload_graph_io_quantization": "0"
            },
            "weight_sharing": true
        }
    },
    "evaluator": "common_evaluator",
    "evaluate_input_model": false,
    "host": "qnn_system",
    "target": "qnn_system",
    "output_dir": "models/google_bert",
    "clean_cache": false,
    "cache_dir": "cache/google_bert/w8a16_ctx"
}
